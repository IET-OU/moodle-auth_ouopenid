{
  "name": "iet-ou/moodle-auth-ouopenid",
  "description": "This plugin is a wrapper around the OpenID authentication plugin for Moodle. Â©2017 The Open University.",
  "homepage": "https://github.com/IET-OU",
  "type": "moodle-auth",
  "license": "proprietary",
  "time": "2017-03-06",
  "support": {
    "source": "https://github.com/IET-OU/moodle-auth_ouopenid"
  },
  "require": {
    "php": ">=5.5.9",
    "composer/installers": "^1.2"
  },
  "require-dev": {
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "squizlabs/php_codesniffer": "2.5.1"
  },
  "scripts": {
    "test": [
      "vendor/bin/parallel-lint --exclude vendor .",
      "composer cn"
    ],
    "phpcs": "vendor/bin/phpcs --standard=./phpcs.xml --ignore=vendor --ignore=.git .",
    "cn": "vendor/bin/phpcs --standard=PHPCS --sniffs=Generic.NamingConventions.ConstructorName --extensions=php --ignore=vendor ."
  },

  "_comment": [
    "https://github.com/squizlabs/PHP_CodeSniffer"
  ]
}
